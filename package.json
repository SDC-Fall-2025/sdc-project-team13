{
  "name": "cache",
  "version": "0.1.0",
  "description": "Cache is a discord bot that helps with managing, leading, and contributing to SDC team projects.",
  "main": "./dist/index.js",
  "type": "commonjs",
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "dev:lint": "esw --watch ./src",
    "dev:compile": "tsx --watch --env-file=.env ./src/index.ts | sed -u 's/\\x1Bc//g'",
    "dev": "conc -c green,blue -n ESLint,TSX 'yarn dev:lint' 'yarn dev:compile'",
    "stylize": "prettier . --write",
    "lint": "eslint ./src",
    "compile": "tsc",
    "build": "yarn lint; yarn compile",
    "start": "node --env-file=.env ./dist/index.js",
    "register:dev": "tsx --env-file=.env ./src/bot/registerCommands.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SDC-Fall-2025/sdc-project-team13.git"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "llm",
    "project",
    "management",
    "project",
    "tool",
    "pm"
  ],
  "author": "SDC Fall 2025 - Team 13",
  "contributors": [
    {
      "name": "Connor Furby",
      "email": "cfurby@wisc.edu"
    },
    {
      "name": "Cash Pergande",
      "email": "cash.pergande@gmail.com"
    },
    {
      "name": "Gavin Smith",
      "email": "me@gavinhsmith.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SDC-Fall-2025/sdc-project-team13/issues"
  },
  "homepage": "https://github.com/SDC-Fall-2025/sdc-project-team13#readme",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@eslint/markdown": "^7.5.0",
    "@types/node": "^24.9.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-watch": "^8.0.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "discord.js": "^14.23.2",
    "octokit": "^5.0.4",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
